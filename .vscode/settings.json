{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".cache": true,
    ".vscode": true,
    "dist": true,
    "node_modules": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.format.enable": true,
  "editor.formatOnSave": true
}
